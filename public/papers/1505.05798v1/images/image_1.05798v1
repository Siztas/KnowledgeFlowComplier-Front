over a (possibly infinite) horizon <math id="S2.SS1.p1.13.m13.1" class="ltx_Math" alttext="M" display="inline"><semantics id="S2.SS1.p1.13.m13.1a"><mi id="S2.SS1.p1.13.m13.1.1" xref="S2.SS1.p1.13.m13.1.1.cmml">M</mi><annotation-xml encoding="MathML-Content" id="S2.SS1.p1.13.m13.1b"><ci id="S2.SS1.p1.13.m13.1.1.cmml" xref="S2.SS1.p1.13.m13.1.1">𝑀</ci></annotation-xml><annotation encoding="application/x-tex" id="S2.SS1.p1.13.m13.1c">M</annotation></semantics></math>. A policy <math id="S2.SS1.p1.14.m14.2" class="ltx_Math" alttext="\pi:\mathcal{X}\times\mathcal{U}\rightarrow[0,1]" display="inline"><semantics id="S2.SS1.p1.14.m14.2a"><mrow id="S2.SS1.p1.14.m14.2.3" xref="S2.SS1.p1.14.m14.2.3.cmml"><mi id="S2.SS1.p1.14.m14.2.3.2" xref="S2.SS1.p1.14.m14.2.3.2.cmml">π</mi><mo lspace="0.278em" rspace="0.278em" id="S2.SS1.p1.14.m14.2.3.1" xref="S2.SS1.p1.14.m14.2.3.1.cmml">:</mo><mrow id="S2.SS1.p1.14.m14.2.3.3" xref="S2.SS1.p1.14.m14.2.3.3.cmml"><mrow id="S2.SS1.p1.14.m14.2.3.3.2" xref="S2.SS1.p1.14.m14.2.3.3.2.cmml"><mi class="ltx_font_mathcaligraphic" id="S2.SS1.p1.14.m14.2.3.3.2.2" xref="S2.SS1.p1.14.m14.2.3.3.2.2.cmml">𝒳</mi><mo lspace="0.222em" rspace="0.222em" id="S2.SS1.p1.14.m14.2.3.3.2.1" xref="S2.SS1.p1.14.m14.2.3.3.2.1.cmml">×</mo><mi class="ltx_font_mathcaligraphic" id="S2.SS1.p1.14.m14.2.3.3.2.3" xref="S2.SS1.p1.14.m14.2.3.3.2.3.cmml">𝒰</mi></mrow><mo stretchy="false" id="S2.SS1.p1.14.m14.2.3.3.1" xref="S2.SS1.p1.14.m14.2.3.3.1.cmml">→</mo><mrow id="S2.SS1.p1.14.m14.2.3.3.3.2" xref="S2.SS1.p1.14.m14.2.3.3.3.1.cmml"><mo stretchy="false" id="S2.SS1.p1.14.m14.2.3.3.3.2.1" xref="S2.SS1.p1.14.m14.2.3.3.3.1.cmml">[</mo><mn id="S2.SS1.p1.14.m14.1.1" xref="S2.SS1.p1.14.m14.1.1.cmml">0</mn><mo id="S2.SS1.p1.14.m14.2.3.3.3.2.2" xref="S2.SS1.p1.14.m14.2.3.3.3.1.cmml">,</mo><mn id="S2.SS1.p1.14.m14.2.2" xref="S2.SS1.p1.14.m14.2.2.cmml">1</mn><mo stretchy="false" id="S2.SS1.p1.14.m14.2.3.3.3.2.3" xref="S2.SS1.p1.14.m14.2.3.3.3.1.cmml">]</mo></mrow></mrow></mrow><annotation-